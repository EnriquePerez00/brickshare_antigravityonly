#%RAML 1.0 DataType
type: object
properties: 
  application:
    type:  string
    required: false
    description: Application invoking the operation
    example: APPNAME
  documentationType:
    type: number
    required: true
    description: Shipment label type to be print  1 (label), 2 (CN22, CN23), 0 (all types)    
    example: 1
  print:
    type: !include labels-impresion-type.raml
    required: false
    description: Label print information
 