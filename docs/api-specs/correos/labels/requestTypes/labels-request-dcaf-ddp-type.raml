#%RAML 1.0 DataType
type: object
properties: 
  application:
    type:  string
    required: false
    description: Application invoking the operation
    example: APPNAME
  documentationType:
    type:  number
    required: true
    description: Documentation type to be printed DCAF (5) or DDP (6)
    example: 5
  documentData:
    type: !include labels-dcaf-ddp-type.raml
    required: false
    description: Document information
 