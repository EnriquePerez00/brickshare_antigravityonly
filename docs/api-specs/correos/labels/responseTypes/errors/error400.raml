#%RAML 1.0 DataType
type: object
properties: 
  code:
    type: string
    required: false
    description: HTTP status code
    example: "400"
  message:
    type: string
    required: false
    description: HTTP Status message
    example: "The request was bad"
  moreInformation:
    type:  !include moreinfo.raml
    required: false
    description: Additional error information 