#%RAML 1.0 DataType
type: object
properties: 
  code:
    type: string
    required: false
    description: HTTP status code
    example: "500"
  message:
    type: string
    required: false
    description: HTTP status message
    example: "There was an error on the server end"
  moreInformation:
    type:  !include moreinfo.raml
    required: false
    description: Additional error information 